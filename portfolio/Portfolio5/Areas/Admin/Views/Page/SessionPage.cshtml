@model Portfolio5.Models.AllSessionPage
@{ ViewData["Title"] = "SessionPage";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml"; }
<br />
<br />
<br />
<div style="min-height: 665.6px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>SessionPage Manager</h1>
                </div>
                <div class="col-sm-6">
                    <br />
                    <ol class=" float-sm-right">
                        <div class="dropdown">
                            <button class="btn btn-primary">
                                Page
                                <i class="fa fa-caret-down"></i>
                            </button>
                            &emsp;&emsp; &emsp;&emsp; &emsp;&emsp;
                            <div class="dropdown-content">
                                <a href="https://www.youtube.com/watch?v=K8hATlKYRMw">Page 1</a>
                                <a href="https://www.youtube.com/watch?v=K8hATlKYRMw">Page 2</a>
                                <a href="https://www.youtube.com/watch?v=K8hATlKYRMw">Page 3</a>
                            </div>
                        </div>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <div class="card-header">
        <h3 class="card-title">Session Page</h3>
    </div>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>IdSess</th>
                <th>IdContentPage</th>
                <th>NameSess</th>
                <th>TitleSess</th>
                <th>ContentSess</th>
                <th>Date Create</th>
                <th>Date update</th>
                <th style="width: 10%">
                    <button type="button" class="btn btn-success " data-toggle="modal" data-target="#sessionpage" onclick="clearTextBox();"><i class="fas fa-plus">ADD</i></button>
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var sessionPage in ViewBag.sessionPages)
            {
                <tr>

                    <td>@sessionPage.IdSess</td>
                    <td>@sessionPage.IdContentPage</td>
                    <td>@sessionPage.NameSess</td>
                    <td>@sessionPage.TitleSess</td>
                    <td>@sessionPage.ContentSess</td>
                    <td>@sessionPage.Datecreated</td>
                    <td>@sessionPage.Dateupdated</td>

                    <td>

                        <a type="button" class="btn btn-success" asp-controller="page" asp-action="updatesessionpage" asp-route-id="@sessionPage.IdSess"><i class="fas fa-edit"></i></a>
                        <a type="button" class="btn btn-danger" asp-controller="page" asp-action="deletesessionpage" asp-route-id="@sessionPage.IdSess" onclick="return confirm('Are you sure delete @sessionPage.IdSess ? ')"><i class="far fa-trash-alt"></i></a>

                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<!-- /.card-body -->

