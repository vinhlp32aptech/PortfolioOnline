<div class="modal fade" id="sessionpage" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
            </div>
            <form method="post" asp-action="addsessionpage" asp-controller="page" enctype="multipart/form-data">
                <div class="modal-body">

                    <div class="form-group">
                        <label for="IdView">IdSess</label>
                        <input type="text" asp-for="SessionPage.IdSess" class="form-control" id="IdView" placeholder="IdSess" />
                    </div>
                    <div class="form-group">
                        <label for="IdView">IdContentPage</label>
                        <input type="text" asp-for="SessionPage.IdContentPage" class="form-control" id="IdView" placeholder="IdContentPage" />
                    </div>
                    <div class="form-group">
                        <label for="IdView">NameSess</label>
                        <input type="text" asp-for="SessionPage.NameSess" class="form-control" id="IdView" placeholder="NameSess" />
                    </div>
                    <div class="form-group">
                        <label for="IdView">TitleSess</label>
                        <input type="text" asp-for="SessionPage.TitleSess" class="form-control" id="IdView" placeholder="TitleSess" />
                    </div>
                    <div class="form-group">
                        <label for="IdView">ContentSess</label>
                        <input type="text" asp-for="SessionPage.ContentSess" class="form-control" id="IdView" placeholder="ContentSess" />
                    </div>             
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary" id="btnAdd" onclick="return Add();">Add</button>


                    @*<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>*@
                </div>
            </form>
        </div>
    </div>
</div>
