@model Portfolio5.Models.Rating
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Edit</h1>


<br />
<br />
<br />
<br />
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6">


                <form method="post" asp-action="edit" asp-controller="rating" enctype="multipart/form-data">

                    <div class="form-group">
                        <label for="Id Rating">Id Rating</label>
                        <input type="text" asp-for="IdRating" class="form-control" id="IdRating" placeholder="Id Rating" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label for="Id Reviewer">Id Reviewer</label>
                        <input type="text" asp-for="IdReviewer" class="form-control" id="IdReviewer" placeholder="Id Reviewer" />
                    </div>
                    <div class="form-group">
                        <label for="Id Account">Id Account</label>
                        <input type="text" asp-for="IdAcc" class="form-control" id="IdAccount" placeholder="Id Account" />
                    </div>
                    <div class="form-group">
                        <label for="StartOfRaing">Start Of Raing</label>
                        <input type="text" asp-for="StarOfRating" class="form-control" id="StartOfRaing" placeholder="Start Of Raing" />
                    </div>
                    <div class="form-group">
                        <label for="NumOfRating">Num Of Rating</label>
                        <input type="text" asp-for="NumOfRating" class="form-control" id="NumOfRating" placeholder="Num Of Rating" />
                    </div>
                    <div class="modal-footer">
                        <input type="hidden" asp-for="IdRating" />
                        <input type="submit" value="Edit" class="btn btn-warning" id="btnEdit" />


                    </div>
                </form>


            </div>

        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</div>